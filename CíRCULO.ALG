Algoritmo "Círculo"

Var

raio, area, valor : real

Inicio

      valor <- 3.14159

      escreval("Digite o valor do raio do circulo: ")
      leia(raio)
      escreval("Digite o valor dado: ")
      leia(valor)

      area <- valor * (raio^2)

      escreval("O valor é: ", area:6:3)

Fimalgoritmo